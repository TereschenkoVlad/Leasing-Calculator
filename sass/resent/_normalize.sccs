body, header, footer, div, span, h1, h2, h3, h4, h5, h6, nav,
ul, li, a, button, section, img, p, script, input {
  margin: 0;
  padding: 0;
}

li {
  list-style: none;

}
a {
  text-decoration: none;
}

button {
  background: none;
  border: 0;
  line-height: normal;
  overflow: visible;
  -webkit-appearance: button;
  -webkit-user-select: none; /* for button */
  -moz-user-select: none;
  -ms-user-select: none;
}